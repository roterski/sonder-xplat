{
  "name": "sonder-xplat",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "start.web.client": "ng serve web-client",
    "build.ionic.client": "cd apps/ionic-client && npm run build:web",
    "prepare.ionic.client": "npm run clean && npm run clean.ionic.client && npm run build.ionic.client",
    "prepare.ionic.client.ios": "npm run prepare.ionic.client && cd apps/ionic-client && npm run cap.add.ios",
    "prepare.ionic.client.android": "npm run prepare.ionic.client && cd apps/ionic-client && npm run cap.add.android",
    "open.ionic.client.ios": "cd apps/ionic-client && npm run cap.ios",
    "open.ionic.client.android": "cd apps/ionic-client && npm run cap.android",
    "sync.ionic.client": "cd apps/ionic-client && npm run cap.copy",
    "start.ionic.client": "cd apps/ionic-client && npm start",
    "clean.ionic.client": "cd apps/ionic-client && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json",
    "start.nativescript.client.ios": "cd apps/nativescript-client && tns run ios --emulator --bundle --env.aot",
    "start.nativescript.client.android": "cd apps/nativescript-client && tns run android --emulator --bundle --env.aot",
    "clean.nativescript.client": "cd apps/nativescript-client && npx rimraf -- hooks node_modules platforms package-lock.json && npm i && npx rimraf -- package-lock.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "~7.0.3",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/material": "~7.0.3",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@datorama/akita": "^1.19.0",
    "@datorama/akita-ng-router-store": "^1.0.1",
    "@datorama/akita-ngdevtools": "^1.0.2",
    "@ionic-native/core": "^5.0.0-beta.15",
    "@ionic-native/splash-screen": "^5.0.0-beta.14",
    "@ionic-native/status-bar": "^5.0.0-beta.14",
    "@ionic/angular": "^4.0.0-beta.3",
    "@ionic/ng-toolkit": "~1.0.0",
    "@ionic/schematics-angular": "~1.0.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngx-translate/core": "~10.0.1",
    "@ngx-translate/http-loader": "~3.0.1",
    "@nrwl/nx": "7.0.2",
    "@sonder/scss": "file:libs/scss",
    "@sonder/web": "file:xplat/web",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-facebook": "^2.4.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26",
    "nativescript-angular": "~6.1.0",
    "nativescript-ngx-fonticon": "^4.2.0",
    "nativescript-theme-core": "^1.0.4",
    "rxjs-compat": "^6.2.2",
    "tns-core-modules": "~4.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "7.0.1",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/schematics": "7.0.2",
    "@nstudio/schematics": "^6.3.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "cypress": "^3.1.0",
    "@types/jquery": "3.3.6",
    "@nrwl/builders": "7.0.2",
    "jest": "^23.0.0",
    "@types/jest": "^23.0.0",
    "jest-preset-angular": "6.0.1",
    "tns-platform-declarations": "~4.2.0"
  },
  "xplat": {
    "prefix": "sonder"
  }
}